{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        ".next/**",
        "!.next/cache/**"
      ]
    },
    "test": {},
    "generate": {},
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "sol:test": {
      "outputs": [
        "coverage/**"
      ]
    },
    "compile": {
      "cache": false
    },
    "start": {
      "cache": false
    }
  }
}