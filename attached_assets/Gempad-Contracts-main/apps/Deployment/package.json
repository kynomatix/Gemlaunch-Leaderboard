{
  "name": "@gempad/deployment",
  "version": "1.0.1",
  "license": "MIT",
  "scripts": {
    "compile": "npx hardhat compile",
    "deploy": "npx hardhat run",
    "getBSCPrice": "CHAIN_ID=5 node scripts/getGasPrice.ts",
    "deploy:goerli": "npx hardhat run  --network goerli ./scripts/deploy.ts",
    "deploy:sepolia": "npx hardhat run  --network sepolia ./scripts/deploy.ts",
    "deploy:fantom": "npx hardhat run  --network fantom ./scripts/deploy.ts",
    "deploy:bscTestnet": "npx hardhat run  --network bscTestnet ./scripts/deploy.ts",
    "deploy:mainnet": "npx hardhat run  --network mainnet ./scripts/deploy.ts",
    "estimateBnbTestnetGas": "node scripts/getBnbGasCost.js",
    "estimateSepoliaGas": "node scripts/getETHGasCost.js",
    "coverage": "npx hardhat coverage",
    "test": "npx hardhat test",
    "unflatten": " npx @devdavi/unflatten",
    "test:coverage": "npx hardhat coverage",
    "flat": "scripts/flat.sh",
    "generate": "node ./scripts/generateContractList.js",
    "generateTestnet": "node ./scripts/generateTestnetContractListt.js",
    "export-abi": "npx hardhat export-abi",
    "clear-abi": "npx hardhat clear-abi",
    "build": "yarn run test && yarn run flat && yarn run export-abi",
    "verify:goerli": "npx hardhat --config hardhat.config.ts run ./scripts/verify.ts --network goerli",
    "verify:mainnet": "npx hardhat --config hardhat.config.ts run ./scripts/deployAndVerify.ts --network mainnet",
    "verify:sepolia": "npx hardhat --config hardhat.config.ts run ./scripts/verify.ts --network sepolia",
    "verify:bscTestnet": "npx hardhat --config hardhat.config.ts run ./scripts/VerifyTestnet.ts --network bscTestnet"
  },
  "dependencies": {
    "@gempad/airdrop": "*",
    "@gempad/antibot": "*",
    "@gempad/dutchauction": "*",
    "@gempad/fairlaunch": "*",
    "@gempad/gempadv2": "*",
    "@gempad/gemtoken": "*",
    "@gempad/generatetokens": "*",
    "@gempad/launchpad": "*",
    "@gempad/locker": "*",
    "@gempad/multisender": "*",
    "@gempad/privatsale": "*",
    "@gempad/services": "*",
    "@gempad/subscriptionpool": "*",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.9",
    "axios": "^1.5.0",
    "eth-token-recover": "^4.9.3",
    "find": "0.3.0",
    "glob": "8.0.3"
  },
  "devDependencies": {
    "@ethersproject/hardware-wallets": "5.6.1",
    "@nomiclabs/hardhat-ethers": "npm:hardhat-deploy-ethers@0.3.0-beta.13",
    "@nomiclabs/hardhat-etherscan": "3.1.0",
    "@types/find": "0.2.1",
    "chalk": "4.1.2",
    "colors": "1.4.0",
    "debug": "4.3.4",
    "ethers": "5.6.9",
    "hardhat": "^2.21.0",
    "hardhat-dependency-compiler": "1.1.3",
    "hardhat-deploy": "0.11.12",
    "hardhat-deploy-ethers": "0.3.0-beta.13",
    "idx": "2.5.6",
    "luxon": "3.0.1"
  },
  "files": [
    "artifacts",
    "deployments/goerli",
    "deployments/bsc",
    "deployments/sepolia",
    "deployments/bscTestnet",
    "contracts.json"
  ]
}